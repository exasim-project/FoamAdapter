FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default         Euler;
    type            backwardEuler;
}

gradSchemes
{
    default         Gauss linear;
    grad(p)         Gauss linear;
}

divSchemes
{
    default         none;
    div(phi,T)      Gauss upwind;
}

laplacianSchemes
{
    //default               none;
    laplacian(kappa,T)      Gauss linear uncorrected;
    laplacian(kappa,nfT)    Gauss linear uncorrected;
    laplacian(Gamma,T)      Gauss linear uncorrected;
}

interpolationSchemes
{
    default         linear;
}

snGradSchemes
{
    default         uncorrected;
}


// ************************************************************************* //
