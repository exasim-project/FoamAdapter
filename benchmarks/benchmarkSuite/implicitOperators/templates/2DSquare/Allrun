#!/bin/sh
cd "${0%/*}" || exit                                # Run from this directory
. ${WM_PROJECT_DIR:?}/bin/tools/RunFunctions        # Tutorial run functions
#------------------------------------------------------------------------------
touch 2dSquare.foam
restore0Dir

runApplication blockMesh

bench_implicitOperators
bench_implicitOperators --reporter xml -o result.xml
runApplication bench_implicitOperators --reporter xml -o result.xml

cat result.xml


#------------------------------------------------------------------------------
