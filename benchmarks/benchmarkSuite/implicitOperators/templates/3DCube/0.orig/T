FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 0.0;

boundaryField
{
    top
    {
        type            fixedValue;
        value           uniform 10.5; // 10.0 will be converted to an int
    }

    bottom
    {
        type            fixedValue;
        value           uniform 1.5; // 1.0 will be converted to an int
    }

    sides
    {
        type            zeroGradient;
    }

    fixedWalls
    {
        type            zeroGradient;
    }

    frontAndBack
    {
        type            empty;
    }
}


// ************************************************************************* //
